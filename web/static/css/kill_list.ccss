table#kills:
	table-layout: fixed
	width: 900px

	tr:nth-child(odd):
		background-color: #1a1a1a

	th.time:
		width: 90px
	th.system:
		width: 120px
	th.victim_portrait:
		width: 64px
	th.victim:
		width: 250px
	th.killer_portrait:
		width: 64px
	th.killer:
		width: 250px
	th.value:
		width: 62px

	td:nth-child(7):
		text-align: right

	.loss:
		color: #a44
